Stepped:Connect(function()
    if a then 
        for i,v in pairs(Character:GetChildren()) do 
            if v:IsA("BasePart") and v.CanCollide then 
                v.CanCollide = false 
            end 
        end
        HumanoidRootPart.Velocity = Vector3.zero
    end 
end)

local shouldattack = false 
local curtween = nil 
section1:AddToggle({
    Name = "Auto Farm", 
    Value = false, 
    Flag = "AFB", 
    Callback = function(state)
        a = state 
        while a do 
            local target = getclosestmob() 
            if target and target[1] and target[2] then 
                if target[1] and (not target[1]:FindFirstChild("HumanoidRootPart")) then 
                    shouldattack = false 
                    if curtween then 
                        curtween:Pause()
                    end 
                    curtween = TweenService:Create(HumanoidRootPart, TweenInfo.new((target[2] - HumanoidRootPart.Position).Magnitude / 200, Enum.EasingStyle.Linear), {CFrame = CFrame.new(target[2])})
                    curtween:Play()
                elseif target[1] and target[1]:FindFirstChild("HumanoidRootPart") then 
                    if (target[1]:FindFirstChild("HumanoidRootPart").Position - HumanoidRootPart.Position).Magnitude < 100 then 
                       --HumanoidRootPart.CFrame = CFrame.new(target[1]:FindFirstChild("HumanoidRootPart").Position + Vector3.new(0, 12, 0) + target[1]:FindFirstChild("HumanoidRootPart").CFrame.LookVector * 6, target[1]:FindFirstChild("HumanoidRootPart").Position)
                        HumanoidRootPart.CFrame = CFrame.new(target[1].HumanoidRootPart.Position + target[1].HumanoidRootPart.CFrame.LookVector * 10, target[1].HumanoidRootPart.Position)
                        shouldattack = true 
                    elseif (target[1]:FindFirstChild("HumanoidRootPart").Position - HumanoidRootPart.Position).Magnitude > 100 then
                        shouldattack = false 
                        if curtween then 
                            curtween:Pause()
                        end 
                        curtween = TweenService:Create(HumanoidRootPart, TweenInfo.new((target[1]:FindFirstChild("HumanoidRootPart").Position - HumanoidRootPart.Position).Magnitude / 200, Enum.EasingStyle.Linear), {CFrame = CFrame.new(target[1]:FindFirstChild("HumanoidRootPart").Position)})
                        curtween:Play()
                    end 
                end 
            end 
            task.wait()
        end 
    end 
})

